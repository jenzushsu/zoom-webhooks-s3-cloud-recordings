{
  "type": "module",
  "name": "zoom-webhooks-s3-cloud-recordings",
  "version": "1.0.0",
  "description": "Sample App demonstrating auto-upload of cloud recordings to AWS S3 using Zoom Webhooks",
  "license": "ISC",
  "author": {
    "name": "Jenzus Hsu",
    "url": "https://developers.zoom.us"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "dev": "nodemon -e ts -w src -x \"tsx ./src/server.ts\"",
    "format:check": "prettier --check ./src/**/*.ts",
    "format:write": "prettier --write ./src/**/*.ts",
    "typecheck": "tsc --noEmit --project ./tsconfig.json"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.556.0",
    "@aws-sdk/lib-storage": "^3.556.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "fastq": "^1.17.1",
    "got": "^14.2.1"
  },
  "devDependencies": {
    "@tsconfig/node-lts": "^20.1.3",
    "@tsconfig/strictest": "^2.0.5",
    "@types/express": "^4.17.21",
    "@types/got": "^9.6.12",
    "@types/node": "^20.12.7",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5"
  }
}
